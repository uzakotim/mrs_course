<?xml version="1.0"?>
<package format="2">

  <name>task_03_gazebo_resources</name>
  <version>1.0.0</version>
  <description>Gazebo resources for task 02 of the MRS course</description>

  <author email="tomas.baca@fel.cvut.cz">Tomas Baca</author>
  <maintainer email="tomas.baca@fel.cvut.cz">Tomas Baca</maintainer>

  <license>BSD 3-Clause</license>

  <buildtool_depend>catkin</buildtool_depend>
  <build_export_depend>boost</build_export_depend>

  <depend>roscpp</depend>
  <depend>nodelet</depend>
  <depend>message_runtime</depend>
  <depend>mrs_lib</depend>
  <depend>geometry_msgs</depend>
  <depend>gazebo</depend>
  <depend>gazebo_ros</depend>
  <depend>gazebo_plugins</depend>

  <export>
    <gazebo_ros gazebo_model_path="${prefix}/models"/>
    <gazebo_ros gazebo_media_path="${prefix}"/>
    <gazebo_ros plugin_path="${prefix}/lib"/>
  </export>

</package>
